{
	"device1": {
		"library": "libroseeros.so",
		"devHandle": "rosNodeHAL",
		"subdevice0": {
			"type": "AnalogOut",
			"channel0": {
				"signalId": "motorEffortOut0",
				"scale": [ { "id" : "dac",
								"scale":	1,
								"offset":	0.0	}
					 ],
				"range": [ { "id" : "dac",
							"minIn":	-9e99,  	"maxIn": 	9e99,
							"minOut":	-10,	"maxOut": 	10 }
					 ],
				"unit": "I",
				"additionalArguments": "topic=/motor_sim/effort; msgType=sensor_msgs::JointState; dataField=effort0"
			}
		}
		"subdevice1": {
			"type": "AnalogIn",
			"channel0": {
				"signalId": "simpleRosIn0",
				"scale": [ { "id" : "dac",
								"scale":	1,
								"offset":	0.0	}
					 ],
				"range": [ { "id" : "dac",
							"minIn":	-9e99,  "maxIn": 	9e99,
							"minOut":	-9e99,	"maxOut": 	9e99 }
					 ],
